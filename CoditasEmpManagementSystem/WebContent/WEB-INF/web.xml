<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CoditasEmpManagementSystem</display-name>
  
  
 <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.coditas.empController.EmpLoginController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>empDetails</servlet-name>
    <servlet-class>com.coditas.empController.EmpDetailsController</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>empDetails</servlet-name>
    <url-pattern>/empDetails</url-pattern>
  </servlet-mapping>
  
  
  
  
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/Error.jsp</location>
  </error-page>
  
  
</web-app>